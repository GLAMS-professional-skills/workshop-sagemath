{
	"name": "SageMath dev",
	// "dockerFile": "Dockerfile", // backup to build container more locally
	"image": "ghcr.com/uoe-som-professional-skils/devcontainer-sagemath",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			],
			"settings": {
				"notebook.displayOrder": [
					"text/latex",
					"text/plain"
				]
			}
		}
	},
	"containerEnv": {
		"SHELL": "/bin/bash"
	}
}
